{"ast":null,"code":"import TasksApi from \"@/api/TasksApi.js\";\nexport default {\n  data: () => {\n    return {\n      summary: {},\n      data: [],\n      newCourse: {\n        name: \"\"\n      },\n      ListaDeTarefas: Array,\n      listaDeCursos: Array,\n      loading: false,\n      exibir: {\n        formGroups: false,\n        listaGroups: true\n      }\n    };\n  },\n  methods: {\n    vaiParaInicio() {\n      this.$router.push(\"/\");\n    },\n\n    listarTarefas(nomeTarefa) {\n      this.$router.push({\n        name: \"TasksList\",\n        params: {\n          filtro: nomeTarefa\n        }\n      });\n    },\n\n    cadastroGrupos() {\n      this.$router.push(\"/groupsForm\");\n    },\n\n    salvarTarefas() {\n      TasksApi.getTasks(data => {\n        this.ListaDeTarefas = data;\n      });\n    },\n\n    calculaQuantidade(nome) {\n      return this.ListaDeTarefas.filter(titulo => titulo.project.includes(nome));\n    }\n\n  },\n\n  created() {\n    this.salvarTarefas();\n\n    if (this.$route.params.name != undefined) {\n      if (this.$route.params.name.length > 0) {\n        this.newCourse.name = this.$route.params.name;\n        this.recebiSalvar(this.newCourse);\n      }\n    }\n  }\n\n};","map":{"version":3,"mappings":"AA4BA;AACA;EACAA;IACA;MACAC,WADA;MAEAD,QAFA;MAGAE;QACAC;MADA,CAHA;MAMAC,qBANA;MAOAC,oBAPA;MAQAC,cARA;MASAC;QACAC,iBADA;QAEAC;MAFA;IATA;EAcA,CAhBA;EAiBAC;IACAC;MACA;IACA,CAHA;;IAIAC;MACA;QAAAT;QAAAU;UAAAC;QAAA;MAAA;IACA,CANA;;IAOAC;MACA;IACA,CATA;;IAUAC;MACAC;QACA;MACA,CAFA;IAGA,CAdA;;IAeAC;MACA,4CACAC,6BADA;IAGA;;EAnBA,CAjBA;;EAsCAC;IACA;;IACA;MACA;QACA;QACA;MACA;IACA;EACA;;AA9CA","names":["data","summary","newCourse","name","ListaDeTarefas","listaDeCursos","loading","exibir","formGroups","listaGroups","methods","vaiParaInicio","listarTarefas","params","filtro","cadastroGrupos","salvarTarefas","TasksApi","calculaQuantidade","titulo","created"],"sourceRoot":"src/views","sources":["ResumePainel.vue"],"sourcesContent":["<template>\n  <v-main class=\"overflow-hidden mt-4\">\n    <v-container>\n      <h2>Resumo das tarefas</h2>\n      <div v-for=\"item in data\" :key=\"item.group\">\n        <h3>\n          {{ item.group }} <v-chip class=\"ma-2\"> {{ item.tasks }} </v-chip>\n        </h3>\n        <v-progress-circular\n          :rotate=\"360\"\n          :size=\"100\"\n          :width=\"15\"\n          :value=\"item.percent\"\n          color=\"pink\"\n        >\n          {{ value }}\n        </v-progress-circular>\n      </div>\n      <div class=\"my-4\">\n        <v-btn rounded color=\"primary\" dark :to=\"{ name: 'taskList' }\">\n          Ver todas tarefas\n        </v-btn>\n      </div>\n    </v-container>\n  </v-main>\n</template>\n\n<script>\nimport TasksApi from \"@/api/TasksApi.js\";\nexport default {\n  data: () => {\n    return {\n      summary: {},\n      data: [],\n      newCourse: {\n        name: \"\",\n      },\n      ListaDeTarefas: Array,\n      listaDeCursos: Array,\n      loading: false,\n      exibir: {\n        formGroups: false,\n        listaGroups: true,\n      },\n    };\n  },\n  methods: {\n    vaiParaInicio() {\n      this.$router.push(\"/\");\n    },\n    listarTarefas(nomeTarefa) {\n      this.$router.push({ name: \"TasksList\", params: { filtro: nomeTarefa } });\n    },\n    cadastroGrupos() {\n      this.$router.push(\"/groupsForm\");\n    },\n    salvarTarefas() {\n      TasksApi.getTasks((data) => {\n        this.ListaDeTarefas = data;\n      });\n    },\n    calculaQuantidade(nome) {\n      return this.ListaDeTarefas.filter((titulo) =>\n        titulo.project.includes(nome)\n      );\n    },\n  },\n  created() {\n    this.salvarTarefas();\n    if (this.$route.params.name != undefined) {\n      if (this.$route.params.name.length > 0) {\n        this.newCourse.name = this.$route.params.name;\n        this.recebiSalvar(this.newCourse);\n      }\n    }\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}