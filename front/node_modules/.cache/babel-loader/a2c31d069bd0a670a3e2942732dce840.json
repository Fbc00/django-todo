{"ast":null,"code":"import TasksApi from \"@/api/TasksApi.js\";\nexport default {\n  data: () => {\n    return {\n      listStatus: [\"done\", \"pending\", \"working\"],\n      ListaDeTarefas: [],\n      loading: false,\n      filtro: \"\",\n      exibir: {\n        formGroups: false,\n        listaGroups: true\n      }\n    };\n  },\n  methods: {\n    listarTarefas() {\n      TasksApi.getTasks(data => {\n        this.ListaDeTarefas = data;\n      });\n    },\n\n    vaiParaInicio() {\n      this.$router.push(\"/\");\n    },\n\n    vaiParaAdicionar() {\n      this.$router.push(\"/newTask\");\n    },\n\n    deletarTask(id) {\n      TasksApi.delTasks(id, () => {\n        this.listarTarefas();\n      });\n    },\n\n    adicionarTask(task) {\n      TasksApi.addTasks(task, () => {\n        this.listarTarefas();\n      });\n    },\n\n    atualizaTask(task) {\n      console.log(task);\n      TasksApi.updateTask(task, () => {\n        this.listarTarefas();\n      });\n    },\n\n    filtroGrupos() {\n      TasksApi.getGroups(data => {\n        for (let curso of data) {\n          this.cursos.push(curso.name);\n          this.cursosId.push(curso.id);\n        }\n      });\n    }\n\n  },\n\n  created() {\n    if (this.$route.params.task != undefined && this.$route.params.task.title != \"\" && this.$route.params.task.project != \"\") {\n      this.adicionarTask(this.$route.params.task);\n    } else {\n      this.filtro = this.$route.params.filtro || \"\";\n      this.listarTarefas();\n    }\n\n    if (this.$route.params.atualizaTarefa != undefined) {\n      this.atualizaTask(this.$route.params.atualizaTarefa);\n    }\n  },\n\n  computed: {\n    filtragem() {\n      return this.ListaDeTarefas.filter(titulo => titulo.project.includes(this.filtro));\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiFA;AACA;EACAA;IACA;MACAC,0CADA;MAEAC,kBAFA;MAGAC,cAHA;MAIAC,UAJA;MAKAC;QACAC,iBADA;QAEAC;MAFA;IALA;EAUA,CAZA;EAaAC;IACAC;MACAC;QACA;MACA,CAFA;IAGA,CALA;;IAMAC;MACA;IACA,CARA;;IASAC;MACA;IACA,CAXA;;IAYAC;MACAH;QACA;MACA,CAFA;IAGA,CAhBA;;IAiBAI;MACAJ;QACA;MACA,CAFA;IAGA,CArBA;;IAsBAK;MACAC;MACAN;QACA;MACA,CAFA;IAGA,CA3BA;;IA4BAO;MACAP;QACA;UACA;UACA;QACA;MACA,CALA;IAMA;;EAnCA,CAbA;;EAkDAQ;IACA,IACA,wCACA,mCADA,IAEA,qCAHA,EAIA;MACA;IACA,CANA,MAMA;MACA;MACA;IACA;;IACA;MACA;IACA;EACA,CAhEA;;EAiEAC;IACAC;MACA,4CACAC,oCADA;IAGA;;EALA;AAjEA","names":["data","listStatus","ListaDeTarefas","loading","filtro","exibir","formGroups","listaGroups","methods","listarTarefas","TasksApi","vaiParaInicio","vaiParaAdicionar","deletarTask","adicionarTask","atualizaTask","console","filtroGrupos","created","computed","filtragem","titulo"],"sourceRoot":"src/views","sources":["TasksList.vue"],"sourcesContent":["<template>\n  <v-container class=\"d-flex flex-column align-center\">\n    <v-container class=\"d-flex justify-space-between\" width=\"970\">\n      <v-btn\n        class=\"align-self-start\"\n        color=\"accent\"\n        elevation=\"2\"\n        rounded\n        x-large\n        :to=\"{ name: 'addTask' }\"\n      >\n        Adicionar\n      </v-btn>\n      <v-btn\n        class=\"align-self-end\"\n        color=\"accent\"\n        elevation=\"2\"\n        rounded\n        x-large\n        :to=\"{ name: 'resume' }\"\n      >\n        Voltar\n      </v-btn>\n      <v-btn\n        class=\"align-self-end\"\n        color=\"accent\"\n        elevation=\"2\"\n        rounded\n        x-large\n        @click=\"vaiParaInicio\"\n      >\n        Logout\n      </v-btn>\n    </v-container>\n    <h1>Lista de Tarefas</h1>\n    <v-text-field\n      v-model=\"filtro\"\n      :counter=\"48\"\n      label=\"Filtrar por grupo\"\n    ></v-text-field>\n    <v-select\n      v-model=\"filtro\"\n      :items=\"listStatus\"\n      label=\"Status\"\n      required\n    ></v-select>\n    <li class=\"d-flex flex-column\">\n      <ul\n        v-for=\"tarefa in filtragem\"\n        :key=\"tarefa.id\"\n        class=\"d-flex mt-8\"\n        fill-heigth\n      >\n        <v-card width=\"550\" class=\"pink lighten-4\">\n          <v-card-title>\n            {{ tarefa.title }}\n          </v-card-title>\n          <v-card-subtitle>\n            {{ tarefa.project }}\n          </v-card-subtitle>\n          <v-card-text>\n            {{ tarefa.date }}\n          </v-card-text>\n          <v-card-actions>\n            <v-btn\n              :to=\"{\n                name: 'taskUpdate',\n                params: { id: tarefa.id, tarefa: tarefa },\n              }\"\n            >\n              Editar\n            </v-btn>\n            <v-btn @click=\"deletarTask(tarefa.id)\">Excluir</v-btn>\n          </v-card-actions>\n        </v-card>\n      </ul>\n    </li>\n  </v-container>\n</template>\n\n<script>\nimport TasksApi from \"@/api/TasksApi.js\";\nexport default {\n  data: () => {\n    return {\n      listStatus: [\"done\", \"pending\", \"working\"],\n      ListaDeTarefas: [],\n      loading: false,\n      filtro: \"\",\n      exibir: {\n        formGroups: false,\n        listaGroups: true,\n      },\n    };\n  },\n  methods: {\n    listarTarefas() {\n      TasksApi.getTasks((data) => {\n        this.ListaDeTarefas = data;\n      });\n    },\n    vaiParaInicio() {\n      this.$router.push(\"/\");\n    },\n    vaiParaAdicionar() {\n      this.$router.push(\"/newTask\");\n    },\n    deletarTask(id) {\n      TasksApi.delTasks(id, () => {\n        this.listarTarefas();\n      });\n    },\n    adicionarTask(task) {\n      TasksApi.addTasks(task, () => {\n        this.listarTarefas();\n      });\n    },\n    atualizaTask(task) {\n      console.log(task);\n      TasksApi.updateTask(task, () => {\n        this.listarTarefas();\n      });\n    },\n    filtroGrupos() {\n      TasksApi.getGroups((data) => {\n        for (let curso of data) {\n          this.cursos.push(curso.name);\n          this.cursosId.push(curso.id);\n        }\n      });\n    },\n  },\n  created() {\n    if (\n      this.$route.params.task != undefined &&\n      this.$route.params.task.title != \"\" &&\n      this.$route.params.task.project != \"\"\n    ) {\n      this.adicionarTask(this.$route.params.task);\n    } else {\n      this.filtro = this.$route.params.filtro || \"\";\n      this.listarTarefas();\n    }\n    if (this.$route.params.atualizaTarefa != undefined) {\n      this.atualizaTask(this.$route.params.atualizaTarefa);\n    }\n  },\n  computed: {\n    filtragem() {\n      return this.ListaDeTarefas.filter((titulo) =>\n        titulo.project.includes(this.filtro)\n      );\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}