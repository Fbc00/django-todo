rio\"\n                required\n                outlined\n                append-icon=\"fa-user\"\n                v-on:keyup.enter=\"login\"\n              ></v-text-field>\n\n              <v-text-field\n                v-model=\"password\"\n                label=\"Senha\"\n                type=\"password\"\n                required\n                outlined\n                append-icon=\"fa-key\"\n                v-on:keyup.enter=\"login\"\n              ></v-text-field>\n\n              <v-btn\n                :loading=\"loading\"\n                :disabled=\"!valid\"\n                color=\"secondary\"\n                class=\"mr-4\"\n                x-large\n                block\n                @click=\"login\"\n                >Continuar <v-icon class=\"pl-3\">fa-arrow-right</v-icon></v-btn\n              >\n            </v-form>\n            <p class=\"ma-4\">\n              <span class=\"subtitle-1\"\n                >Não tenho conta! Fazer <a href=\"\">Cadastro</a></span\n              >\n            </p>\n          </v-card>\n        </v-col>\n      </v-row>\n    </v-container>\n  </v-container>\n</template>\n\n<script>\nexport default {\n  methods: {\n    cadastroFeito() {\n      this.$router.push(\"/login\");\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}