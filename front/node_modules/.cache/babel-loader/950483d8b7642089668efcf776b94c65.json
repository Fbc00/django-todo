v-card width=\"550\" class=\"pink lighten-4\">\n          <v-card-title>\n            {{ tarefa.title }}\n          </v-card-title>\n          <v-card-subtitle>\n            {{ tarefa.project }}\n          </v-card-subtitle>\n          <v-card-text>\n            {{ tarefa.date }}\n          </v-card-text>\n          <v-card-actions>\n            <v-btn\n              :to=\"{\n                name: 'taskUpdate',\n                params: { id: tarefa.id, tarefa: tarefa },\n              }\"\n            >\n              Editar\n            </v-btn>\n            <v-btn>Excluir</v-btn>\n          </v-card-actions>\n        </v-card>\n      </ul>\n    </li>\n  </v-container>\n</template>\n\n<script>\nimport TasksApi from \"@/TasksApi.js\";\nexport default {\n  data: () => {\n    return {\n      ListaDeTarefas: Array,\n      loading: false,\n      exibir: {\n        formGroups: false,\n        listaGroups: true,\n      },\n    };\n  },\n  methods: {\n    vaiParaInicio() {\n      this.$router.push(\"/\");\n    },\n    vaiParaAdicionar() {\n      this.$router.push(\"/newTask\");\n    },\n  },\n  created() {\n    TasksApi.getTasks((data) => {\n      this.ListaDeTarefas = data;\n    });\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}